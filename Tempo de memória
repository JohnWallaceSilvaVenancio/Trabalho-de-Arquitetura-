#include <stdio.h>
#include <stdlib.h>
#include <time.h>
 
int main(){
	FILE *arq;
	arq = fopen("Benchmark.txt","rt");
	if(arq == NULL){
		printf("Problema na existencia do arquivo!\n");
	}
    int i;
    printf("intervalo da rand: [0,%d]\n", RAND_MAX);
    for(i=1 ; i <= 100 ; i++){
        printf("Numero %d: %d\n",i, rand());
 	}
}
